# RekvizituValdymoSistema/.htaccess
<IfModule mod_rewrite.c>
  RewriteEngine On

  # Nukreipti visus užklausimus į public katalogą
  RewriteRule ^$ public/ [L]
  RewriteRule (.*) public/$1 [L]
</IfModule>

